{
    "name": "wikia/docker-php",
    "description": "PHP client for Docker API generated from Docker's OpenAPI spec",
    "type": "library",
    "require": {
        "jane-php/open-api-runtime": "^6.0"
    },
    "require-dev": {
        "jane-php/open-api-2": "^6.0"
    },
    "autoload": {
       "psr-4": {
            "Docker\\Api\\": "src/"
       }
    },
    "scripts": {
        "generate": "jane-openapi generate --config-file=config/jane-openapi-configuration.php"
    },
    "license": "Apache-2.0"
}
